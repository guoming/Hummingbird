{
  "$schema": "http://cachemanager.michaco.net/schemas/cachemanager.json#",
  "redis": [
    {
      "key": "redisConnection",
      "database": 0,
      "connectionString": "localhost:6379,password=,allowAdmin=true,ssl=false,abortConnect=false,connectTimeout=5000"
    }
  ],
  "cacheManagers": [
    {
      "maxRetries": 1000,
      "name": "HummingbirdCache",
      "retryTimeout": 100,
      "updateMode": "Up",
      //"backplane": {
      //  "key": "redisConnection",
      //  "knownType": "Redis",
      //  "channelName": "redisChannel"
      //},
      "loggerFactory": {
        "knownType": "Microsoft"
      },
      "serializer": {
        "knownType": "Json"
      },
      "handles": [
        {
          "knownType": "MsMemory",
          "enablePerformanceCounters": true,
          "enableStatistics": true,
          "expirationMode": "Absolute",
          "expirationTimeout": "0:0:23",
          "isBackplaneSource": false,
          "name": "sys cache"
        }
        //{
        //  "knownType": "Redis",
        //  "key": "redisConnection",
        //  "isBackplaneSource": true
        //}
      ]
    }
  ]
}